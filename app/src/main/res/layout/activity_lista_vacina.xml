<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.petshow.activity.ListaVacinaActivity"
    android:background="@color/BegieFundoPet"
    android:orientation="vertical"
    app:statusBarBackground="@color/VerdeFundoPet">

  <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar_activity_lista_vacina"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/VerdeFundoPet"
            />

    </android.support.design.widget.AppBarLayout>

    <include layout="@layout/menu_superior" />

    <include layout="@layout/content_lista_vacina" />

</LinearLayout>
